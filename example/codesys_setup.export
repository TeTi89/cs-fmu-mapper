      <Single Name="Object" Type="{6f9dac99-8de1-4efc-8465-68ac443b7d08}" Method="IArchivable">
        <Single Name="SpecialFunc" Type="{0db3d7bb-cde0-4416-9a7b-ce49a0124323}">None</Single>
        <Single Name="Implementation" Type="{3b83b776-fb25-43b8-99f2-3c507c9143fc}" Method="IArchivable">
          <Single Name="TextDocument" Type="{f3878285-8e4f-490b-bb1b-9acbb7eb04db}" Method="IArchivable">
            <Single Name="TextBlobForSerialisation" Type="string">CASE state OF
	SimulationState.IDLE:
		calculationFinished := FALSE;
		terminate := FALSE;
		doStep := FALSE;
		IF run THEN
			state := SimulationState.STEP_TRIGGER;
		END_IF
		
	SimulationState.STEP_TRIGGER:
		calculationFinished := FALSE;
		IF run THEN
			doStep := TRUE;
			state := SimulationState.STEP_CALCULATION;
		ELSE 
			state := SimulationState.IDLE;
		END_IF
	SimulationState.STEP_CALCULATION:
		IF NOT run THEN
			state := SimulationState.IDLE;
		ELSIF calculationFinished THEN
			doStep := FALSE;
			state := SimulationState.STEP_TRIGGER;
		ELSIF doStep THEN
			state := SimulationState.ERROR;
		END_IF
	SimulationState.ERROR:
		_pApp := AppGetCurrent(pResult := ADR(_Result));
		AppGenerateException(pApp:=_pApp,   ulException:=RtsExceptions.RTSEXCPT_CYCLE_TIME_EXCEED);
END_CASE</Single>
            <Single Name="LineInfoPersistence" Type="string">ed986165-82c7-4e55-b19a-5a3b6a12a5ac_Impl_LineIds</Single>
          </Single>
        </Single>
