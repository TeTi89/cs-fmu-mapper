          <Single Name="TextDocument" Type="{f3878285-8e4f-490b-bb1b-9acbb7eb04db}" Method="IArchivable">
            <Single Name="TextBlobForSerialisation" Type="string">PROGRAM SimulationWatchdog
VAR_INPUT
	terminate: BOOL;
	run: BOOL := TRUE;
END_VAR
VAR
    //The next two are for getting App instance
    _pApp       : POINTER TO CmpApp.APPLICATION;
    _Result     : SysExcept.SysTypes.RTS_IEC_RESULT;
	
	state: SimulationState := SimulationState.IDLE;
	doStep: BOOL;
	calculationFinished: BOOL;
END_VAR</Single>
